    --disable-cursor \
    --disable-micemoveable \
    --disable-dblclk \
    --enable-debug \
    --enable-msgstr \
    --disable-nativegal \
    --disable-textmode \
    --disable-videoqvfb \
    --disable-videoecoslcd \
    --disable-videodummy \
    --disable-nativeial \
    --disable-vbfsupport \
    --disable-qpfsupport \
    --disable-ttfsupport \
    --disable-type1support \
    --disable-gbsupport \
    --disable-big5support \
    --disable-gifsupport \
    --disable-jpgsupport \
    --disable-imegb2312py \
    --build=i386-linux
